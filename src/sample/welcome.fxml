<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.WelcomeController">
   <children>
      <ImageView fitHeight="450.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/welcome.jpg" />
         </image>
      </ImageView>
      <Button layoutX="30.0" layoutY="225.0" mnemonicParsing="false" onAction="#startProgram" prefHeight="32.0" prefWidth="190.0" text="Click here to get Started!" textAlignment="CENTER" wrapText="true" />
      <Label layoutX="33.0" layoutY="161.0" prefHeight="51.0" prefWidth="183.0" text="Welcome to ICC Cricket World Cup 2022" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
   </children>
</AnchorPane>
